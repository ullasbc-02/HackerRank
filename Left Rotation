def rotateLeft(d, arr):
    # Write your code here
    temp=0
    for j in range(d):
        temp = arr[0]
        print(temp)
        for i in range(len(arr) - 1):
            arr[i] = arr[i + 1]
        arr[len(arr) - 1] = temp
    return arr
    #Time limit exceeds
    
    
    #Best approach
    def rotateLeft(d, arr):
      return(arr[d:] + arr[:d])
