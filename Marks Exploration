def marsExploration(s):
    # Write your code here
    count = 0
    for i in range(len(s)):
        if(i%3==0):
            if(s[i]!='S'):
                count = count + 1
        if(i%3==1):
            if(s[i]!='O'):
                count = count + 1
        if(i%3==2):
            if(s[i]!='S'):
                count = count + 1
    return count
